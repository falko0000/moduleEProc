<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="tj.organization.other.info">
	<namespace>org_other_info</namespace>

	<entity local-service="true" name="OrgOtherInfo" table="sapp.org_otherinfo" remote-service="false">
		<column name="organization_id" type="long" primary="true"></column>
		<column name="mesto_registracii" type="String"></column>
		<column name="kratkoe_naimenovanie" type="String"></column>
		<column name="vbk_id" type="long"></column>
		<column name="fbk_id" type="long"></column>
		<column name="istochnik_finansirovanija_id" type="int"></column>
		<column name="kvalificirovannaja_organizacija" type="boolean"></column>
		<column name="data_gosudarstvennoj_registracii" type="Date"></column>
		<column name="rajon_id" type="long"></column>
		<column name="inn" type="String"></column>
		<column name="ein" type="String"></column>
		<column name="sin" type="String"></column>
		<column name="sozdal" type="long"></column>
		<column name="data_sozdanija" type="Date"></column>
		<column name="izmenil" type="long"></column>
		<column name="data_izmenenija" type="Date"></column>

      <finder return-type="OrgOtherInfo" name="inn">
      	<finder-column name="inn"></finder-column>
      </finder>
	  <finder return-type="OrgOtherInfo" name="OrgOtherInfoId">
	  <finder-column name="organization_id"></finder-column>
	  </finder>
	</entity>
</service-builder>