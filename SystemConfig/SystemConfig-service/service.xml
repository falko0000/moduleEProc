<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="tj.system.config">
	<namespace>System_config</namespace>
	
	<entity local-service="true" name="SystemConfig" remote-service="false" uuid="false" table="sapp.system_config">
	
		<column name="system_config_id_" type="long" primary="true" id-type="increment"></column>
		<column name="key_" type="String"></column>
		<column name="description" type="String"></column>
		<column name="value" type="String"></column>
		<column name="type_" type="String"></column>
		
			<finder return-type="SystemConfig" name="Key">
			<finder-column name="key"></finder-column>
			</finder>


	</entity>
</service-builder>