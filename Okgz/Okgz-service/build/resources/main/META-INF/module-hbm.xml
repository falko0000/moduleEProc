<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="tj.okgz.model.Okgz" />
	<class name="tj.okgz.model.impl.OkgzImpl" table="klassifikatory.okgz">
		<id name="okgz_id" type="long">
			<generator class="sequence">
				<param name="sequence">klassifikatory.okgz_id_seq</param>
			</generator>
		</id>
		<property name="kod" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="naimenovanie" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>