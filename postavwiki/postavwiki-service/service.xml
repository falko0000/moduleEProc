<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="tj.postavwiki">
	<namespace>postavwiki</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
    <entity name="Postavwiki" local-service="true" table="sapp.postavwiki">
        <column name="adres" type="String"></column>
        <column name="data_gosudarstvennoj_registracii" type="Date"></column>
        <column name="data_izmenenija" type="Date"></column>
        <column name="data_sozdanija" type="Date"></column>
        <column name="ein" type="String"></column>
        <column name="gorod" type="String"></column>
        <column name="inn" type="String"></column>
        <column name="izmenil" type="long"></column>
        <column name="kratkoe_naimenovanie" type="String"></column>
        <column name="mesto_registracii" type="String"></column>
        <column name="oblast_id" type="long"></column>
        <column name="polnoe_naimenovanie" type="String"></column>
        <column name="postavwiki_id" type="long" primary="true" id-type="sequence" id-param="seq_postawki"></column>
        <column name="rajon_id" type="long"></column>
        <column name="sin" type="String"></column>
        <column name="sozdal" type="long"></column>
        
        	<finder return-type="Postavwiki" name="postavwikiIdOblastId">
        	<finder-column name="postavwiki_id"></finder-column>
        	<finder-column name="oblast_id"></finder-column>        	
        	</finder>
        	
    </entity>
</service-builder>