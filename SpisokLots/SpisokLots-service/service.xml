<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="tj.spisoklotov">
	<namespace>tj_spisoklotov</namespace>
	<entity name="Spisoklotov" local-service="true" table="sapp.spisok_lotov" remote-service="false">
        <column name="cena_kontrakta" type="double"></column>
        <column name="cena_postavki" type="String"></column>
        <column name="data_izmenenija" type="Date"></column>
        <column name="data_sozdanija" type="Date"></column>
        <column name="id_jebk" type="long"></column>
        <column name="istochnik_finansirovanija_id" type="long"></column>
        <column name="izmenil" type="long"></column>
        <column name="izvewenie_id" type="long"></column>
        <column name="klassifikacija_po_jekb" type="String"></column>
        <column name="kod_jekb" type="String"></column>
        <column name="kod_zakazchika" type="String"></column>
        <column name="mesto_postavki" type="String"></column>
        <column name="naimenovanie_lota" type="String"></column>
        <column name="nomer_lota" type="long"></column>
        <column name="obespechenie_ispolnenija" type="long"></column>
        <column name="obespechenie_ispolnenija_v_procentah" type="double"></column>
        <column name="obespechenie_v_procentah" type="double"></column>
        <column name="obespechenie_zajavki" type="long"></column>
        <column name="oplata" type="String"></column>
        <column name="soputstvujuwie_uslovija" type="String"></column>
        <column name="sozdal" type="long"></column>
        <column name="spisok_lotov_id" type="long" primary="true" id-type="sequence" id-param="sapp.spisok_lotov_id_seq"></column>
        <column name="srok_dejstvija" type="String"></column>
        <column name="srok_ispolnenija_zajavki" type="String"></column>
        <column name="srok_obespechenija_zajavki" type="String"></column>
        <column name="srok_postavki" type="String"></column>
        <column name="zakazchik" type="String"></column>
        
        <finder return-type="Collection" name="IzvewenieID">
        	<finder-column name="izvewenie_id"></finder-column>
        </finder>
        
    </entity>

</service-builder>